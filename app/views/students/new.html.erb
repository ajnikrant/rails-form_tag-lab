<h1> Students New Page</h1>
<%= form_tag students_path do %>
    <label> Student Form</label> <br>
    <textarea id="student first name" name="student[first_name]"></textarea><br>
    
    <textarea id="student last name" name="student[last_name]"></textarea><br>
    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <input type="submit" value="Submit Student">
    <% end %> 